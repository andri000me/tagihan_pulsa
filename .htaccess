#cegah anonymous
<Files .htaccess>
    order allow,deny
    deny from all
</Files>

#aturan url dashboard
<IfModule mod_rewrite.c>
    RewriteEngine on
    RewriteRule ^login index.php?halaman=login [NC,L]
    RewriteRule ^daftar index.php?halaman=daftar [NC,L]
    RewriteRule ^whatsapp index.php?halaman=whatsapp [NC,L]
</IfModule>